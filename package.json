    "@hapi/eslint-config-hapi": "^12.0.0",
    "@hapi/eslint-plugin-hapi": "^4.3.4",
    "blipp": "^4.0.1",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5"
  }
}
