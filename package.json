    "@hapi/eslint-plugin-hapi": "^4.3.5",
    "@hapi/lab": "^22.0.4",
    "blipp": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "npm-run-all": "^4.1.5"
