  },
  "keywords": [
    "hapi",
    "eventlogging"
  ],
  "dependencies": {
    "@hapi/glue": "^6.1.1",
    "@hapi/good": "^8.2.3",
    "@hapi/good-console": "^8.1.1",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^18.3.0",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^15.1.1",
    "@hapi/nes": "^11.2.1",
    "@hapi/vision": "^5.5.4",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "hapi-auth-jwt2": "^8.6.2",
    "hapi-mongodb": "^9.0.0",
