  "license": "MIT",
  "description": "Sealog event-logging server",
  "bugs": {
    "url": "https://github.com/webbpinner/sealog-server/issues"
  },
  "main": "server.js",
  "scripts": {
    "test": "./node_modules/lab/bin/lab -v --leaks --timeout 8000",
    "start": "NODE_ENV=production node server.js",
